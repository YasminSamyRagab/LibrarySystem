@model IEnumerable<MemberBook>
@{
    ViewBag.Title = "ShowArrivedBooks";
}

<h2>ShowArrivedBooks</h2>

<ul class="list-group">

    @foreach (var item in Model)
    {
        if (item.Book.PublishingDate.Year < DateTime.Now.Year)
        {
            <li class="list-group-item list-group-item-primary">@item.Book.Title </li>
        }

        else
        {
            <li class="list-group-item list-group-item-danger"> No New Books Arrived</li>
        }
    }

</ul>

@Html.ActionLink("Back", "ViewProfile", "Shimaa", null, new { @class = "btn btn-primary" })


